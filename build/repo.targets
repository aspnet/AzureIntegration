<Project>
  <!-- pack site extension last -->
  <Target Name="ReorderProjectsToPack" BeforeTargets="PackageProjectsToPack">
    <ItemGroup>
      <_SiteExtensions Include="$(RepositoryRoot)src\*.SiteExtension\*.csproj" />
      <ProjectsToPack Remove="@(_SiteExtensions)" />
      <ProjectsToPack Include="@(_SiteExtensions)"  />
    </ItemGroup>
  </Target>
</Project>
